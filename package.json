        "db:studio": "drizzle-kit studio",
        "db:dev:migrate": "dotenv -c development -- yarn db:migrate",
        "db:prod:migrate": "dotenv -c production -- yarn db:migrate",
        "format:check": "prettier --check \"**/*.{ts,tsx}\"",
