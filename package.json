{
    "name": "@cartesi/explorer",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "dev": "run-s db:dev:migrate next:dev",
        "build": "run-s db:prod:migrate next:build",
        "start": "next start",
        "next:build": "npm run chakra-types && next build",
        "next:dev": "npm run chakra-types && next dev --turbopack",
        "test": "dotenv -c test -- jest",
        "test:ci": "dotenv -c test -- jest --runInBand --coverage",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:dev:migrate": "dotenv -c development -- yarn db:migrate",
        "db:prod:migrate": "dotenv -c production -- yarn db:migrate",
        "format:check": "prettier --check \"**/*.{ts,tsx}\"",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "lint": "eslint --color --ext .ts --ext .tsx src/",
        "lint:fix": "yarn lint --fix",
        "chromatic": "chromatic --exit-zero-on-changes",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "chakra-types": "chakra typegen ./src/styles/theme.ts"
    },
